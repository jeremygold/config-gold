[user]
	name = Jeremy Gold
	email = grobbledorf@gmail.com
[push]
	default = current
[color]
	ui = true
	status = auto
	branch = auto
[core]
	editor = vim
[merge]
	tool = meld
[alias]
    # l = "log --tags --all --graph --date=short --pretty=format:'%C(ul #777777)%h%C(noul #4444AA) (%cd) %C(yellow)%an %C(cyan)%d%Creset %s'"
    l = log --tags --all --graph --date=short --pretty=format:'%C(yellow)%h %C(blue)(%cd) %C(yellow)%an %C(cyan)%d%Creset %s'
	ci = commit
    st = status
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
